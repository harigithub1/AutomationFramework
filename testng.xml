<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Myntra-Android-Test-Suite" parallel="tests" thread-count="5">
    <!--udid value(eg:RZ8NA0RM5GK) can be given in deviceName parameter in case of local testing-->

    <test name="Local S20">
        <parameter name="deviceModel" value="S20"/>
        <parameter name="deviceName" value="RZ8NA0RM5GK"/>
        <parameter name="platformVersion" value="12.0"/>
        <classes>
            <class name="cucumber.tests.CucumberTestNGParallelRunner1WithMultiThreading"/>
        </classes>
    </test>

    <test name="Local A71">
        <parameter name="deviceModel" value="A71"/>
        <parameter name="deviceName" value="RZ8NC07WQNP"/>
        <parameter name="platformVersion" value="11.0"/>
        <classes>
            <class name="cucumber.tests.CucumberTestNGParallelRunner2WithMultiThreading"/>
        </classes>
    </test>

